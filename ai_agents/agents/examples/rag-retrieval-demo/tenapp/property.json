{
  "ten": {
    "predefined_graphs": [
      {
        "name": "rag_retrieval_graph",
        "auto_start": true,
        "graph": {
          "nodes": [
            {
              "type": "extension",
              "name": "rag_retrieval",
              "addon": "rag_retrieval_python",
              "extension_group": "rag",
              "property": {
                "vector_db_path": "./data/vector_db",
                "embedding_model": "sentence-transformers/all-MiniLM-L6-v2",
                "chunk_size": 512,
                "chunk_overlap": 50,
                "top_k": 5,
                "similarity_threshold": 0.7
              }
            }
          ],
          "connections": []
        }
      }
    ],
    "log": {
      "handlers": [
        {
          "matchers": [
            {
              "level": "info"
            }
          ],
          "formatter": {
            "type": "plain",
            "colored": true
          },
          "emitter": {
            "type": "file",
            "config": {
              "path": "logs/app.log",
              "max_size": 10485760,
              "max_backups": 5
            }
          }
        },
        {
          "matchers": [
            {
              "level": "info"
            }
          ],
          "formatter": {
            "type": "plain",
            "colored": true
          },
          "emitter": {
            "type": "console",
            "config": {
              "stream": "stdout"
            }
          }
        }
      ]
    }
  }
}